%dw 2.0
import withMaxSize from dw::core::Strings
output application/json
fun formatDataStructure( myfield ) =
myfield match {
case is Null -> null
case is String ->   (myfield as String {format: "YYYY-MM-DD HH24:MI:SS"})[0 to 18]
}

